{
  "name": "insert-project-name",
  "version": "1.0.0",
  "description": "INSERT PROJECT DESCRIPTION>",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./",
    "dev": "export $(cat .dev.env | xargs) && npm run generate:cert && npm run dev:docker && npm run start",
    "dev:watch": "nodemon -e 'ts' -i '**/*.test.ts' -w 'src/*' -x 'npm run dev'",
    "dev:docker": "docker-compose kill && docker-compose rm -f && docker-compose down -v && docker-compose build && docker-compose up -d",
    "test": "concurrently -p '路{name}路' -n UNIT,LINT -c cyan.inverse,magenta.inverse 'npm:test:unit' 'npm:lint'",
    "test:unit": "export $(cat .test.env | xargs) && jest",
    "test:watch": "npm run test:unit -- --watch",
    "lint": "concurrently -p '路{name}路' -n CODE,COMMITS -c magenta.inverse 'npm:lint:code' 'npm:lint:commits'",
    "lint:code": "eslint --ext .tsx,.ts .",
    "lint:commits": "commitlint -f $(git log --reverse --pretty=%H | head -1) -t $(git log -1 --pretty=%H)",
    "build": "tsc"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "env-var": "^6.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  }
}
