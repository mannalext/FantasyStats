{
  "name": "fantasy-stats",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev:fresh": "npm run build && npm run dev:docker && node ./dist/app.js",
    "dev": "npm run build && npm run start",
    "start": "NODE_ENV=development node --inspect=0.0.0.0 ./dist/app.js",
    "build": "tsoa spec-and-routes && tsc -b tsconfig.build.json",
    "dev:watch": "nodemon --ignore src/routes.ts -e 'ts' -i '**/*.test.ts' -w 'src/*' -x 'npm run dev'",
    "dev:docker": "docker-compose kill && docker-compose rm -f && docker-compose down -v && docker-compose build && docker-compose up -d",
    "lint": "yarn run eslint . --ext ts --max-warnings=0",
    "format": "npx prettier --write **/*.ts",
    "everything-under-this-is-up-for-deletion": "node",
    "testOLD": "concurrently -p '路{name}路' -n UNIT,LINT -c cyan.inverse,magenta.inverse 'npm:test:unit' 'npm:lint'",
    "test:unit": "export $(cat .test.env | xargs) && jest",
    "test:watch": "npm run test:unit -- --watch",
    "linOLD": "concurrently -p '路{name}路' -n CODE,COMMITS -c magenta.inverse 'npm:lint:code' 'npm:lint:commits'",
    "lint:code": "eslint --ext .tsx,.ts .",
    "lint:commits": "commitlint -f $(git log --reverse --pretty=%H | head -1) -t $(git log -1 --pretty=%H)",
    "test": "USE_FAKE_PORTS='true' jest --group=-bq",
    "testWatch": "USE_FAKE_PORTS='true' jest --watch",
    "testRealPorts": "npm run dev:docker && jest --runInBand",
    "testWatchRealPorts": "npm run dev:docker && jest --watch --runInBand",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@koa/router": "^10.1.1",
    "axios": "^0.27.2",
    "env-var": "^6.2.0",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.3",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa2-swagger-ui": "^5.5.1",
    "pg": "^8.7.3",
    "tsoa": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@types/jest": "^27.0.2",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-static": "^4.0.2",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "concurrently": "^5.2.0",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^26.5.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "jest": "^27.3.0",
    "jest-junit": "^13.2.0",
    "lint-staged": "^13.0.0",
    "madge": "^5.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.6.2",
    "ts-jest": "^27",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
